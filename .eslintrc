{
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "rules": {
        "react/jsx-filename-extension": [ 1, { "extensions": [".js"] } ],
        "indent": ["error", 4],
        "react/jsx-indent": ["error", 4],
        "import/prefer-default-export": "off",
        "import/no-extraneous-dependencies": "off"
    }
}
