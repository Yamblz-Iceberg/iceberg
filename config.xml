<?xml version='1.0' encoding='utf-8'?>
<widget id="ru.yandex.cordova" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>cordova-boilerplate</name>
    <description>
    A sample Apache Cordova application that responds to the deviceready event.
  </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
    Apache Cordova Team
  </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/icon/android/android-icon-36x36.png" />
        <icon density="mdpi" src="res/icon/android/android-icon-48x48.png" />
        <icon density="hdpi" src="res/icon/android/android-icon-72x72.png" />
        <icon density="xhdpi" src="res/icon/android/android-icon-96x96.png" />
        <icon density="xxhdpi" src="res/icon/android/android-icon-144x144.png" />
        <icon density="xxxhdpi" src="res/icon/android/android-icon-192x192.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <icon height="57" src="res/icon/ios/apple-icon-57x57.png" width="57" />
        <icon height="60" src="res/icon/ios/apple-icon-60x60.png" width="60" />
        <icon height="72" src="res/icon/ios/apple-icon-72x72.png" width="72" />
        <icon height="76" src="res/icon/ios/apple-icon-76x76.png" width="76" />
        <icon height="114" src="res/icon/ios/apple-icon-114x114.png" width="114" />
        <icon height="120" src="res/icon/ios/apple-icon-120x120.png" width="120" />
        <icon height="144" src="res/icon/ios/apple-icon-144x144.png" width="144" />
        <icon height="152" src="res/icon/ios/apple-icon-152x152.png" width="152" />
        <icon height="180" src="res/icon/ios/apple-icon-180x180.png" width="180" />
    </platform>
    <preference name="AndroidPersistentFileLocation" value="Compatibility" />
    <preference name="AndroidExtraFilesystems" value="sdcard,cache" />
    <preference name="Orientation" value="portrait" />
    <engine name="android" spec="^6.2.3" />
    <engine name="browser" spec="^4.1.0" />
    <engine name="ios" spec="^4.4.0" />
    <plugin name="cordova-plugin-camera" spec="^2.4.1">
        <variable name="CAMERA_USAGE_DESCRIPTION" value=" " />
        <variable name="PHOTOLIBRARY_USAGE_DESCRIPTION" value=" " />
    </plugin>
    <plugin name="cordova-plugin-customurlscheme" spec="^4.3.0">
        <variable name="URL_SCHEME" value="iceberg" />
        <variable name="ANDROID_SCHEME" value=" " />
        <variable name="ANDROID_HOST" value=" " />
        <variable name="ANDROID_PATHPREFIX" value="/" />
    </plugin>
    <plugin name="cordova-plugin-file" spec="^4.3.3" />
    <plugin name="cordova-plugin-file-transfer" spec="^1.6.3" />
    <plugin name="cordova-plugin-safariviewcontroller" spec="^1.4.7" />
    <plugin name="cordova-plugin-statusbar" spec="^2.2.3" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>
